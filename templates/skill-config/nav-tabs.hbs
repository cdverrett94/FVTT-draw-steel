<div class='sheet-tabs tabs' data-group='main'>
    {{#each skillsList as |category|}}
        {{#unless (eq @key 'customSkills')}}
            <a class='nav-tab' data-action='tab' data-group='main' data-tab='{{@key}}'>
                {{localize (concat 'system.skills.' @key '.label')}}
            </a>
        {{/unless}}
    {{/each}}
    <a class='nav-tab' data-action='tab' data-group='main' data-tab='customSkills'>
        {{localize 'system.skills.customSkills.label'}}
    </a>
</div>