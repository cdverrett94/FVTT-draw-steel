<div class='sheet-tabs tabs' data-group='main'>
    <a
        class='nav-tab {{ifThen (or (eq activeTabs.main "abilities") (eq activeTabs.main null)) "active" ""}}'
        data-action='tab'
        data-group='main'
        data-tab='abilities'
    >{{localize 'system.items.ability.label.plural'}}</a>
    <a class='nav-tab {{ifThen (eq activeTabs.main "notes") "active" ""}}' data-action='tab' data-group='main' data-tab='notes'>{{localize
            'system.sheets.actor.notes'
        }}</a>
    <a class='nav-tab {{ifThen (eq activeTabs.main "effects") "active" ""}}' data-action='tab' data-group='main' data-tab='effects'>{{localize
            'system.general.effect.plural'
        }}</a>
</div>